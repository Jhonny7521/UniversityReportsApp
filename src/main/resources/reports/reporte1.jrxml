	<queryString language="SQL">
		<![CDATA[SELECT S.ID, S.NAME, S.NUMBEROFSTUDENTS, S.TAXRESOURCE, 
CASE S.LICENCED
	WHEN  1 THEN 'Si' 
	ELSE 'No' 
END AS LICENCED,
S.CLASIFICATION, F.DESCRIPTION, 
TO_CHAR(S.CREATEDAT, 'DD/MM/YYYY') AS CREATEDAT
FROM SCHOOLS S
INNER JOIN FACULTIES F 
ON S.FACULTIESID = F.ID]]>
	</queryString>
